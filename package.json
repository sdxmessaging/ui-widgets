{
  "name": "ui-widgets",
  "private": true,
  "version": "1.0.0",
  "description": "ui-widgets",
  "repository": {
    "type": "git",
    "url": "https://github.com/sdxmessaging/ui-widgets.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -b",
    "postbuild": "rollup -c",
    "watch": "tsc -w",
    "bundle-watch": "rollup -c -w",
    "preexample": "tsc -b",
    "example": "rollup -c rollup.config.example.js",
    "open": "open example.html",
    "test": "ospec src/**/*.spec.ts --require ts-node/register",
    "coverage": "nyc ospec src/**/*.spec.ts --require ts-node/register --require source-map-support/register"
  },
  "author": "",
  "license": "UNLICENSED",
  "types": "./lib/index.d.ts",
  "dependencies": {
    "@types/lodash": "^4.14.136",
    "@types/mithril": "^1.1.16",
    "@types/signature_pad": "^2.3.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/node": "^12.6.3",
    "lodash": "^4.17.14",
    "mithril": "^1.1.6",
    "nyc": "^14.1.1",
    "ospec": "^3.1.0",
    "rollup": "^1.17.0",
    "rollup-plugin-terser": "^5.1.1",
    "signature_pad": "^3.0.0-beta.3",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "text",
      "html"
    ]
  }
}
